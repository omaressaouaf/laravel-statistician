{
    "name": "omaressaouaf/laravel-statistician",
    "version": "1.0.0",
    "description": "",
    "keywords": [
        "omaressaouaf",
        "php",
        "laravel",
        "statistics",
        "stats",
        "dashboard"
    ],
    "type": "library",
    "homepage": "https://github.com/omaressaouaf/laravel-statistician",
    "license": "MIT",
    "authors": [
        {
            "name": "Omar Essaouaf",
            "email": "souafomar@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omaressaouaf\\LaravelStatistician\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omaressaouaf\\LaravelStatistician\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.5",
        "laravel/pint": "^1.21"
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Omaressaouaf\\LaravelStatistician\\StatisticianServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
